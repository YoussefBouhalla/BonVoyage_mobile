{
  "expo": {
    "name": "BonVoyage_mobile",
    "slug": "BonVoyage_mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "splash": {
        "mdpi": "./assets/img/splash/splash-mdpi.png",
        "hdpi": "./assets/img/splash/splash-hdpi.png",
        "xhdpi": "./assets/img/splash/splash-xhdpi.png",
        "xxhdpi": "./assets/img/splash/splash-xxhdpi.png",
        "xxxhdpi": "./assets/img/splash/splash-xxxhdpi.png",
        "resizeMode": "cover",
        "backgroundColor": "#EB5353"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
